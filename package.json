{
  "name": "mui-cascade",
  "version": "1.0.25",
  "description": "A React component library built with MUI (Material-UI) - a shadcn alternative",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "docs",
    "bin",
    "src/components",
    "src/theme",
    "src/index.js"
  ],
  "bin": {
    "mui-cascade-add": "./bin/mui-cascade-add.js"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "dev": "rollup -c -w",
    "dev:standalone": "vite",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "release:patch": "yarn version patch && yarn publish",
    "release:minor": "yarn version minor && yarn publish",
    "release:major": "yarn version major && yarn publish"
  },
  "peerDependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@mui/system": "^5.14.20",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "keywords": [
    "react",
    "mui",
    "material-ui",
    "components",
    "library",
    "shadcn-alternative"
  ],
  "author": "MiniMax Agent",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/mui-cascade.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/mui-cascade/issues"
  },
  "homepage": "https://github.com/your-username/mui-cascade#readme",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@mui/system": "^5.14.20",
    "chalk": "^5.6.2",
    "commander": "^11.1.0",
    "fs-extra": "^11.3.2",
    "inquirer": "^8.2.6",
    "livecodes": "^0.12.0",
    "lucide-react": "^0.554.0",
    "monaco-editor": "^0.55.1",
    "tslib": "^2.8.1"
  }
}
